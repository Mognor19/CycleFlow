{
  "expo": {
    "name": "CycleFlow",
    "slug": "CycleFlow",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/CycleFlow.png",
    "splash": {
      "image": "./assets/splash.jpg",
      "resizeMode": "fill",
      "backgroundColor": "#f35454"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "host.exp.apple"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "host.exp.platforms"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
